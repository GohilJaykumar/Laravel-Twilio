{
    "name": "laravel/twilio",
    "type": "library",
    "description": "A Laravel package for Twilio's API",
    "keywords": ["twilio", "sms", "api", "laravel"],
    "homepage": "https://github.com/GohilJaykumar/Laravel-Twilio",
    "license": "MIT",
    "authors": [
        {
            "name": "Gohil Jaykumar",
            "email": "gohiljaykumar1996@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1.0"
    },
    "require-dev": {
        "guzzlehttp/guzzle": "^6.3 || ^7.0",
        "phpunit/phpunit": ">=7.0"
    },
    "suggest": {
        "guzzlehttp/guzzle": "An HTTP client to execute the API requests"
    },
    "autoload": {
        "psr-4": {
            "Twilio\\": "src/Twilio/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Twilio\\Tests\\": "tests/Twilio/"
        }
    },
    "config": {
        "lock": false
    }
}
